.about-credits
  bottom 18px
  display block
  font-size 13px
  font-weight 100
  letter-spacing .1em
  position absolute
  right 15px
  text-align right
  z-index 10

  @media (max-width: 1080px)
    display none

.about-credits-icon
  display inline-block
  fill #FFF
  height 25px
  margin-bottom 10px
  width 25px
